{
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "bugs": {
    "url": "https://github.com/Artsdatabanken/nin-kart-frontend/issues"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@sentry/browser": "^5.7.1",
    "classnames": "^2.2.6",
    "eslint-plugin-react-hooks": "^4.2.0",
    "leaflet": "^1.6.0",
    "markdown-to-jsx": "^6.11.4",
    "pixl-xml": "^1.0.13",
    "proj4": "^2.6.2",
    "qs": "^6.9.4",
    "react": "^16.11.0",
    "react-color": "^2.17.3",
    "react-dom": "^16.11.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.0",
    "react-use": "^12.10.0",
    "recompose": "^0.30.0",
    "dexie": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "coveralls": "^3.0.7",
    "eslint": "7.15.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "@testing-library/react": "^11.2.2",
    "@testing-library/jest-dom": "^5.11.6",
    "cypress": "^6.2.1",
    "cypress-file-upload": "^4.1.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "prettier",
      "jsx-a11y",
      "react-hooks"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": 2,
      "react-hooks/rules-of-hooks": 2,
      "jsx-a11y/alt-text": 1
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/tools/fileMock.js",
      "\\.(css|less)$": "<rootDir>/tests/tools/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/cypress"
    ]
  },
  "license": "MIT",
  "name": "nin-kart-frontend",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "build-storybook": "build-storybook -s public -o build/storybook",
    "eject": "react-scripts eject",
    "deploy": "scp -r build/* grunnkart@hydra:~/okologiskegrunnkart/",
    "report-coverage": "coveralls < coverage/lcov.info",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "jest",
    "test:watch": "jest --watch",
    "e2e": "cypress run",
    "e2e:test": "cypress run --env baseurl=https://okologiskegrunnkart.test.artsdatabanken.no",
    "e2e:prod": "cypress run --env baseurl=https://okologiskegrunnkart.artsdatabanken.no",
    "open": "cypress open",
    "open:test": "cypress open --env baseurl=https://okologiskegrunnkart.test.artsdatabanken.no",
    "open:prod": "cypress open --env baseurl=https://okologiskegrunnkart.artsdatabanken.no",
    "group1": "cypress run --spec cypress/integration/drawPolygon.test.js,cypress/integration/showTiles.test.js,cypress/integration/switchFavorites.test.js,cypress/integration/urlUpdate.test.js",
    "group2": "cypress run --spec cypress/integration/savePolygon.test.js,cypress/integration/uploadPolygon.test.js,cypress/integration/searchBar.test.js",
    "group3": "cypress run --spec cypress/integration/clickAllLayers.test.js,cypress/integration/clickSelectedLayers.test.js,cypress/integration/editFavorites.test.js",
    "group1:test": "cypress run --spec cypress/integration/drawPolygon.test.js,cypress/integration/showTiles.test.js,cypress/integration/switchFavorites.test.js,cypress/integration/urlUpdate.test.js --env baseurl=https://okologiskegrunnkart.test.artsdatabanken.no",
    "group2:test": "cypress run --spec cypress/integration/savePolygon.test.js,cypress/integration/uploadPolygon.test.js,cypress/integration/searchBar.test.js --env baseurl=https://okologiskegrunnkart.test.artsdatabanken.no",
    "group3:test": "cypress run --spec cypress/integration/clickAllLayers.test.js,cypress/integration/clickSelectedLayers.test.js,cypress/integration/editFavorites.test.js --env baseurl=https://okologiskegrunnkart.test.artsdatabanken.no",
    "group1:prod": "cypress run --spec cypress/integration/drawPolygon.test.js,cypress/integration/showTiles.test.js,cypress/integration/switchFavorites.test.js,cypress/integration/urlUpdate.test.js --env baseurl=https://okologiskegrunnkart.artsdatabanken.no",
    "group2:prod": "cypress run --spec cypress/integration/savePolygon.test.js,cypress/integration/uploadPolygon.test.js,cypress/integration/searchBar.test.js --env baseurl=https://okologiskegrunnkart.artsdatabanken.no",
    "group3:prod": "cypress run --spec cypress/integration/clickAllLayers.test.js,cypress/integration/clickSelectedLayers.test.js,cypress/integration/editFavorites.test.js --env baseurl=https://okologiskegrunnkart.artsdatabanken.no"
  },
  "version": "0.2.0"
}
